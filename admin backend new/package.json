{
  "name": "jewelry-admin-backend-ts",
  "version": "1.0.0",
  "description": "TypeScript Backend API for Jewelry Admin Dashboard",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "seed": "ts-node src/scripts/seedSuperAdmin.ts",
    "create-user": "ts-node src/scripts/createTestUser.ts",
    "seed-defaults": "ts-node src/scripts/seedDefaultValues.ts",
    "seed-customization": "ts-node src/scripts/seedCustomizationRequests.ts",
    "seed-orders": "ts-node src/scripts/seedOrders.ts",
    "test": "jest"
  },
  "keywords": [
    "jewelry",
    "admin",
    "dashboard",
    "api",
    "nodejs",
    "express",
    "typescript",
    "mongodb"
  ],
  "author": "Jewelry Admin Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
